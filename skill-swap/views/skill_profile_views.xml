<odoo>
    <record id="view_form_skill_profile" model="ir.ui.view">
        <field name="name">skill.profile.form</field>
        <field name="model">skill.profile</field>
        <field name="arch" type="xml">
            <form string="Skill Profile">
                <group>
                    <field name="name"/>
                    <field name="location"/>
                    <field name="profile_photo"/>
                    <field name="skills_offered"/>
                    <field name="skills_wanted"/>
                    <field name="availability"/>
                    <field name="is_public"/>
                </group>
            </form>
        </field>
    </record>

    <record id="view_tree_skill_profile" model="ir.ui.view">
        <field name="name">skill.profile.tree</field>
        <field name="model">skill.profile</field>
        <field name="arch" type="xml">
            <tree string="Skill Profiles">
                <field name="name"/>
                <field name="skills_offered"/>
                <field name="skills_wanted"/>
                <field name="availability"/>
                <field name="is_public"/>
            </tree>
        </field>
    </record>

    <record id="action_skill_profile" model="ir.actions.act_window">
        <field name="name">Skill Profiles</field>
        <field name="res_model">skill.profile</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>
